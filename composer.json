{
    "name": "larastash/reviews",
    "description": "Eloquent model reviews for Laravel.",
    "keywords": [
        "laravel",
        "laravel-reviews",
        "laravel-reviewable",
        "reviews",
        "reviewable",
        "eloquent",
        "model"
    ],
    "homepage": "https://github.com/larastash/reviews",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Larastash\\Reviews\\": "src"
        },
        "files": ["helpers.php"]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larastash\\Reviews\\Providers\\ReviewsServiceProvider"
            ]
        }
    }
}